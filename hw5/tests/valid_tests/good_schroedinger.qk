class Schroedinger(box: Int) {
    if box > 0 {
       this.living = true;
    } else {
       this.living = false;
    }

    def is_alive() : Boolean {
        return this.living;
    }
}

x = Schroedinger(3);
if (x.is_alive()){
    "Alive".PRINT();
}
else{
    "Dead".PRINT();
}
"\n".PRINT();

y = Schroedinger(-3);
if (y.is_alive()){
    "Alive".PRINT();
}
else{
    "Dead".PRINT();
}
"\n".PRINT();
