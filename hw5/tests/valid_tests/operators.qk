/* Test Builtin Operators */
newline = "\n";

// test integer builtins
"\nTest Integer Builtins:\n".PRINT();
intA = 1;
intA.PRINT();
"  <-- correct: 1".PRINT();
newline.PRINT();
intB = 2;
intB.PRINT();
"  <-- correct: 2".PRINT();
newline.PRINT();
(intA +  intB).PRINT();
"  <-- correct: 3".PRINT();
newline.PRINT();
(intA -  intB).PRINT();
"  <-- correct: -1".PRINT();
newline.PRINT();
(intA *  intB).PRINT();
"  <-- correct: 2".PRINT();
newline.PRINT();
(intA /  intB).PRINT();
"  <-- correct: 0".PRINT();
newline.PRINT();
(intA <  intB).PRINT();
"  <-- correct: true".PRINT();
newline.PRINT();
(intA <= intB).PRINT();
"  <-- correct: true".PRINT();
newline.PRINT();
(intA == intB).PRINT();
"  <-- correct: false".PRINT();
newline.PRINT();
(intA > intB).PRINT();
"  <-- correct: false".PRINT();
newline.PRINT();
(intA >= intB).PRINT();
"  <-- correct: false".PRINT();
newline.PRINT();

// test string builtins
"\nTest String Builtins:\n".PRINT();
strA = "foo";
strA.PRINT();
"  <-- correct: foo".PRINT();
newline.PRINT();
strB = "bar";
strB.PRINT();
"  <-- correct: bar".PRINT();
newline.PRINT();
(strA +  strB).PRINT();
"  <-- correct: foobar".PRINT();
newline.PRINT();
(strA < strB).PRINT();
"  <-- correct: false".PRINT();
newline.PRINT();
(strA <= strB).PRINT();
"  <-- correct: false".PRINT();
newline.PRINT();
(strA == strB).PRINT();
"  <-- correct: false".PRINT();
newline.PRINT();
(strA >  strB).PRINT();
"  <-- correct: true".PRINT();
newline.PRINT();
(strA >= strB).PRINT();
"  <-- correct: true".PRINT();
newline.PRINT();

// test boolean builtins
"\nTest Boolean Builtins:\n".PRINT();
boolA = true;
boolA.PRINT();
"  <-- correct: true".PRINT();
newline.PRINT();
boolB = false;
boolB.PRINT();
"  <-- correct: false".PRINT();
newline.PRINT();
(boolA and boolB).PRINT();
"  <-- correct: false".PRINT();
newline.PRINT();
(boolA or boolB).PRINT();
"  <-- correct: true".PRINT();
newline.PRINT();
(not boolA).PRINT();
"  <-- correct: false".PRINT();
newline.PRINT();

// Test String Comparisons
"\nTest String Comparison:\n".PRINT();
a = "a";
b = "b";
c = a+b;

"a = ".PRINT(); a.PRINT(); "\n".PRINT();
"b = ".PRINT(); b.PRINT(); "\n".PRINT();
"c = ".PRINT(); c.PRINT(); "\n".PRINT();

("a < b:   " + (a < b).STR()     + " <-- correct: true"  + "\n").PRINT(); 
("a > b:   " + (a > b).STR()     + " <-- correct: false" + "\n").PRINT(); 
("a <= b:  " + (a <= b).STR()    + " <-- correct: true"  + "\n").PRINT();
("a >= b:  " + (a >= b).STR()    + " <-- correct: false" + "\n").PRINT();
("a == b:  " + (a == b).STR()    + " <-- correct: false" + "\n").PRINT();
("c < a:   " + (c < a).STR()     + " <-- correct: false" + "\n").PRINT();
("c < b:   " + (c < b).STR()     + " <-- correct: true"  + "\n").PRINT();
("c > a:   " + (c > a).STR()     + " <-- correct: true"  + "\n").PRINT();
("c > b:   " + (c > b).STR()     + " <-- correct: false" + "\n").PRINT();
("c >= a:  " + (c >= a).STR()    + " <-- correct: true"  + "\n").PRINT();
("c >= b:  " + (c >= b).STR()    + " <-- correct: false" + "\n").PRINT();
("c <= a:  " + (c <= a).STR()    + " <-- correct: false" + "\n").PRINT();
("c <= b:  " + (c <= b).STR()    + " <-- correct: true"  + "\n").PRINT();
("c == a:  " + (c == a).STR()    + " <-- correct: false" + "\n").PRINT();
("c == ab: " + (c == "ab").STR() + " <-- correct: true" + "\n").PRINT();
