class Pt(x: Int, y: Int) {
   this.x = x;
   this.y = y;

   def PRINT( ): Nothing {
      "(".PRINT();
      this.x.PRINT();
      ", ".PRINT();
      this.y.PRINT();
      ")".PRINT();
    }

    def PLUS(other: Pt):Pt {
        return Pt(this.x+other.x, this.y+other.y);
    }
}

x = 11;
y = 31;


pt1 = Pt(x,y);
pt1.PRINT();
"\n".PRINT();
pt2 = Pt(y,x);
pt2.PRINT();
"\n".PRINT();
pt3 = pt1 + pt2;
pt3.PRINT();
"\n".PRINT();
