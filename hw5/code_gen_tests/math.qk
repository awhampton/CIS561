/* computes some math functions on integers */

class Math() {

    // returns x mod m
    def mod(x : Int, m : Int) : Int {
        if( m < 2){
            return -1;
        }

        q = x / m;
        return x - q * m;
    }

    // returns x!
    def factorial(x : Int) : Int {
        if(x < 0){
            return -1;
        }
        else{
            res = 1;
            while(x > 0){
                res = res * x;
                x = x - 1;
            }
            return res;
        }
    }

    // return x ^ y (try to implement with squaring?)
    def power(x : Int, y : Int) : Int {
        if(y < 0){
            return -1;
        }
        else{
            res = 1;
            while(y > 0){
                res = res * x;
                y = y - 1;
            }
            return res;
        }
    }

    // return floor(sqrt(x))
    def sqrt(x : Int) : Int {
        return -1;
    }

}

